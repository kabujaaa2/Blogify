{
  "name": "blogify",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode production && open dist/stats.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "server": "node src/server.js",
    "server:dev": "nodemon --watch src --ext js,mjs,cjs,json src/server.js",
    "start": "./start-prod.sh",
    "dev:all": "./start-dev.sh",
    "deps": "npm install tailwindcss@latest @tailwindcss/vite@latest @tailwindcss/postcss@latest @tailwindcss/typography@latest",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "docker:build": "docker build -t blogify:latest .",
    "docker:run": "docker run -p 12000:12000 -p 12001:12001 --env-file .env blogify:latest",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
